pipeline {
    agent any
    stages {
        stage ("SCM Chkout-1"){
            steps {
                git 'https://github.com/Vishwanathms/Java-Proj1'
            }
        }
        stage ("Java execute"){
            steps {
                sh 'javac *.java'
                sh 'java Simple'
                sh 'ls -l'
                sh 'scp *.class root@192.168.1.24:/root/projects'
            }
        }
    }
}
